{
  "builds": [
    {
      "src": "career_cast/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "installCommand": "chmod +x build.sh && ./build.sh",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "career_cast/wsgi.py"
    }
  ],
  "functions": {
    "career_cast/wsgi.py": {
      "maxDuration": 30
    }
  }
}
